<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="PaymentScreenInherit" t-inherit="point_of_sale.PaymentScreen"
       t-inherit-mode="extension" owl="1">
       <xpath expr="//div[hasclass('payment-controls')]" position="inside">
            <div id="button_test_pos" class="button js_invoice"
                t-on-click="showPopupTest">
                <i class="fa fa-spinner fa-spin"/> Click me
            </div>
        </xpath>
    </t>
</templates>